import time
import matplotlib.pyplot as plt
import numpy as np

def graficar(xs, ys):
    
    xs = np.array(xs)
    ys = np.array(ys)

    plt.plot(xs, ys, lw=2, marker="o",linestyle='--',color="#ff3333",label='32 ROBOTS')
    
    plt.xticks(xs)
    plt.yticks(ys)
    
    plt.title("EBW - Evaluaci칩n cl치sica")
    plt.xlabel("Tama침o de grafo bipartito")
    plt.ylabel("Tiempo de ejecuci칩n (ns)")
    plt.grid()
    plt.legend()
    plt.savefig('ebw_clasica.png')
    plt.show()
    
xs = ["K10,10","K15,15","K20,20","K25,25","K30,30","K35,35","K40,40"]
ys = [2900,9800,34730,76610,81030,230100,230269]
    
# create data
#graficar(xc1,yc1,xc2,yc2,xc4,yc4,xc8,yc8,xc16,yc16)
graficar(xs, ys)
